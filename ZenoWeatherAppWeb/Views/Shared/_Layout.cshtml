    <nav class="navbar navbar-expand-lg navbar-dark bg-dark shadow fixed-top" aria-label="Offcanvas navbar large">
        <div class="container-fluid">
            <a class="navbar-brand" asp-area="" asp-page="/Index" id="navbarBrandText">ZenoWeather</a>
            <div class="d-flex align-middle" style="margin-right: 8px">
                <div class="d-flex justify-content-between" style="margin-left: 0px;">
                    <p id="weatherProvidedBy">
                        Weather Data<br />Provided by:
                    </p>
                </div>
                <a href="https://www.accuweather.com/" target="_blank" title="Visit https://www.accuweather.com/" role="button" class="list-group-item d-flex " aria-current="true">
                    <img src="@Url.Content("~/Assets/AccuWeather Light.png")" alt="twbs" height="16" class="img-responsive center-block flex-shrink-0" style="margin-top: 4px;">
                </a>
            </div>
            <form class="d-flex" role="search" style="margin-right: 8px" method="post">
                <input class="form-control me-2" id="weatherSearchInput" type="search" placeholder="Enter a Location..." aria-label="Search" style="min-width: 128px; max-width: 180px; font-weight: bold; height: 46px;" onkeyup="searchWeather(event)">
                <button class="btn btn-primary" type="submit">Search</button>
            </form>
            <button class="navbar-toggler" title="Menu" type="button" data-bs-toggle="offcanvas" data-bs-target="#offcanvasNavbar2" aria-controls="offcanvasNavbar2">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="offcanvas offcanvas-end text-bg-dark" tabindex="-1" id="offcanvasNavbar2" aria-labelledby="offcanvasNavbar2Label">
                <div class="offcanvas-header">
                    <h5 class="offcanvas-title" id="offcanvasNavbar2Label">Zeno Weather</h5>
                    <button type="button" class="btn-close btn-close-white" data-bs-dismiss="offcanvas" aria-label="Close"></button>
                </div>
                <div class="offcanvas-body">
                    <ul class="navbar-nav justify-content-end flex-grow-1 pe-3" id="mainMenuItems">
                        <li class="nav-item">

                            <a asp-area="" asp-controller="Home" asp-action="Index" aria-label="Weather" class="nav-link is-active-page" aria-current="page"><svg class="bi pe-none" width="32" height="32" role="img" aria-label="Weather"><use xlink:href="#weather"></use></svg> Weather</a>
                        </li>
                        <li class="nav-item">

                            <a asp-area="" asp-controller="Home" asp-action="Settings" aria-label="Settings" class="nav-link is-active-page"><svg class="bi pe-none" width="32" height="32" role="img" aria-label="Settings"><use xlink:href="#cogs"></use></svg> Settings</a>
                        </li>
                        <li class="nav-item">

                            <a asp-area="" asp-controller="Home" asp-action="About" aria-label="About" class="nav-link is-active-page"><svg class="bi pe-none" width="32" height="32" role="img" aria-label="About"><use xlink:href="#question"></use></svg> About</a>
                        </li>
                        <li class="nav-item">
                            <a href="https://github.com/DivideByZeno/Zeno-Weather-App" aria-label="GitHub" class="nav-link is-active-page"><svg class="bi pe-none" width="32" height="32" role="img" aria-label="GitHub"><use xlink:href="#github"></use></svg> GitHub</a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </nav>

    <!--Container Main start-->
    <main role="main">
        <div class="d-flex flex-column">
            @RenderBody()
        </div>
    </main>

    <script src="/lib/jquery/dist/jquery.min.js"></script>
    <script src="/lib/bootstrap/dist/js/bootstrap.min.js" defer></script>
    <script src="~/js/site.js" asp-append-version="true" defer></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
